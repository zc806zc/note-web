{
    "language": "zh-hans",
    "plugins": [
       "splitter",
       "toggle-chapters",
       "disqus",
       "tbfed-pagefooter",
       "-search",
       "search-pro"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "gitbookuse"
        },
        "tbfed-pagefooter": {
            "modify_label": "最后修改 : ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        }
    }
}